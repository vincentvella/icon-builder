{
  "expo": {
    "name": "icon-builder",
    "slug": "icon-builder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "https://icon-builder.vellapps.com/icon?color=%2306b6d4&emoji=1f3d7",
      "resizeMode": "contain",
      "backgroundColor": "#06b6d4"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://icon-builder.vellapps.com/splash?color=%2306b6d4&emoji=1f3d7",
        "backgroundColor": "#06b6d4"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "https://icon-builder.vellapps.com/favicon?color=%2306b6d4&emoji=1f3d7",
      "output": "server"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
